{
    "id": "air_con_hub",
    "name":
    {
        "en": "Air Conditioner (HUB Remote)"
    },
    "class": "remote",
    "capabilities": [
        "power_on",
        "power_off",
        "target_temperature",
        "aircon_mode",
        "aircon_fan_speed"
    ],
    "pair": [
    {
        "id": "list_my_devices",
        "template": "list_devices",
        "navigation":
        {
            "next": "add_my_devices"
        }
    },
    {
        "id": "add_my_devices",
        "template": "add_devices"
    }],
    "images":
    {
        "large": "/drivers/air_con_hub/assets/images/large.jpg",
        "small": "/drivers/air_con_hub/assets/images/small.jpg"
    }
}